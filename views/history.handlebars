{{!-- THIS IS THE HEADER OF THE PAGE --}}

<div class="container-fluid">
  <div class="row">
    <div class="col-xs-2 col-md-3">
    </div>  
    <h1 class="col-xs-8 col-md-6 text-center header">Here are all the previous topics:</h1>
    </div>  
